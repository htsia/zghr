<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
    
    <managed-bean>
      <managed-bean-name>rule_infobb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.RuleInfoBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>ruleucc</property-name>
            <value>#{rule_infoucc}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
      <managed-bean-name>law_infobb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.LawInfoBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>lawucc</property-name>
            <value>#{law_infoucc}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
      <managed-bean-name>rule_browbb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.RuleBrowBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>ruleucc</property-name>
            <value>#{rule_infoucc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
      <managed-bean-name>rule_editbb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.RuleEditBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>ruleucc</property-name>
            <value>#{rule_infoucc}</value>
        </managed-property>
    </managed-bean>


    <managed-bean>
      <managed-bean-name>law_browbb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.LawBrowBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>lawucc</property-name>
            <value>#{law_infoucc}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
      <managed-bean-name>law_editbb</managed-bean-name>
        <managed-bean-class>com.hr319wg.rule.web.LawEditBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>lawucc</property-name>
            <value>#{law_infoucc}</value>
        </managed-property>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/rule/LawDictEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/rule/LawFileEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/rule/RuleDictEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/rule/RuleFileEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>

