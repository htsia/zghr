<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

   <managed-bean>
        <managed-bean-name>cle_DateListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.college.web.DateListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>collegeucc</property-name>
            <value>#{cle_collegeucc}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
         <managed-bean-name>cle_ClassListBB</managed-bean-name>
         <managed-bean-class>com.hr319wg.college.web.ClassListBackingBean</managed-bean-class>
         <managed-bean-scope>request</managed-bean-scope>
         <managed-property>
             <property-name>collegeucc</property-name>
             <value>#{cle_collegeucc}</value>
         </managed-property>
     </managed-bean>

    </faces-config>
