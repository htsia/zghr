<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

    <managed-bean>
        <managed-bean-name>qry_namelistBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.NameListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>namelistucc</property-name>
            <value>#{qry_nameUCCImpl}</value>
        </managed-property>
        <managed-property>
            <property-name>qrypersonlistucc</property-name>
            <value>#{emp_qryPersonListUCC}</value>
        </managed-property>
  </managed-bean>

    <managed-bean>
        <managed-bean-name>qry_queryClassBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.QueryClassBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>classucc</property-name>
            <value>#{qry_queryClassUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>qry_queryBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.QueryBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>queryucc</property-name>
            <value>#{qry_queryUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>qry_querySettingBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.QuerySettingBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>queryucc</property-name>
            <value>#{qry_queryUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>userqryucc</property-name>
            <value>#{user_userQryUccImpl}</value>
        </managed-property>
        <managed-property>
            <property-name>userreportucc</property-name>
            <value>#{user_userReportUccImpl}</value>
        </managed-property>

    </managed-bean>

    <managed-bean>
        <managed-bean-name>qry_qryModuleSettingBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.QryModuleSettingBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>userqryucc</property-name>
            <value>#{user_userQryUccImpl}</value>
        </managed-property>
        <managed-property>
            <property-name>usermanucc</property-name>
            <value>#{user_userManageUccImpl}</value>
        </managed-property>
        <managed-property>
            <property-name>queryucc</property-name>
            <value>#{qry_QueryUCCImpl}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>qry_staticSettingBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.qry.web.StaticSettingBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>queryucc</property-name>
            <value>#{qry_queryUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>rpt_queueListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.common.queue.QueueListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>queueucc</property-name>
            <value>#{qry_queueUCC}</value>
        </managed-property>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/qry/*</from-view-id>
        <navigation-case>
            <from-outcome>close</from-outcome>
            <to-view-id>/pages/qry/autoclose.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>queryresult</from-outcome>
            <to-view-id>/qry/QueryResult.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>staticresult</from-outcome>
            <to-view-id>/qry/StaticResult.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/qry/QueryOrgSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>condition</from-outcome>
            <to-view-id>/qry/QueryConditionSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>staticlist</from-outcome>
            <to-view-id>/qry/StaticListSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/qry/QueryConditionSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>org</from-outcome>
            <to-view-id>/qry/QueryOrgSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>item</from-outcome>
            <to-view-id>/qry/QueryItemSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>static</from-outcome>
            <to-view-id>/qry/QueryStaticSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/qry/QueryItemSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>condition</from-outcome>
            <to-view-id>/qry/QueryConditionSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>close</from-outcome>
            <to-view-id>/pages/qry/autoclose.htm</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/qry/QueryStaticSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/qry/QueryConditionSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/qry/StaticListSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>org</from-outcome>
            <to-view-id>/qry/QueryOrgSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>staticedit</from-outcome>
            <to-view-id>/qry/StaticConditionSetting.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>staticitem</from-outcome>
            <to-view-id>/qry/StaticItemSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/qry/StaticConditionSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>staticlist</from-outcome>
            <to-view-id>/qry/StaticListSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/qry/StaticItemSetting.jsp</from-view-id>
        <navigation-case>
            <from-outcome>staticlist</from-outcome>
            <to-view-id>/qry/StaticListSetting.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
