<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

    <managed-bean>
       <managed-bean-name>org_reportBB</managed-bean-name>
       <managed-bean-class>com.hr319wg.org.web.OrgReportBackingBean</managed-bean-class>
       <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgreportucc</property-name>
            <value>#{org_ReportUCC}</value>
        </managed-property>
   </managed-bean>

    <managed-bean>
       <managed-bean-name>org_DataUploadBB</managed-bean-name>
       <managed-bean-class>com.hr319wg.org.web.DataUploadBackingBean</managed-bean-class>
       <managed-bean-scope>request</managed-bean-scope>
       <managed-property>
           <property-name>infosetucc</property-name>
           <value>#{sys_infoSetUccImpl}</value>
       </managed-property>
        <managed-property>
            <property-name>datauploaducc</property-name>
            <value>#{emp_datauploadUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>showitemucc</property-name>
            <value>#{sys_showitemsUCC}</value>
        </managed-property>
   </managed-bean>

    <managed-bean>
       <managed-bean-name>org_changeInfoBB</managed-bean-name>
       <managed-bean-class>com.hr319wg.org.web.ChangeInfoBackingBean</managed-bean-class>
       <managed-bean-scope>request</managed-bean-scope>
       <managed-property>
           <property-name>orgUCC</property-name>
           <value>#{org_orgUCC}</value>
       </managed-property>
        <managed-property>
            <property-name>userreportucc</property-name>
            <value>#{user_userReportUccImpl}</value>
        </managed-property>
   </managed-bean>

    <managed-bean>
        <managed-bean-name>OrgListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>userreportucc</property-name>
            <value>#{user_userReportUccImpl}</value>
        </managed-property>
        
    </managed-bean>

    <managed-bean>
        <managed-bean-name>UpPhBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.UploadPhotoBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>personucc</property-name>
            <value>#{emp_personUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>attachucc</property-name>
            <value>#{common_attachmentUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>OrgAddBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgAddBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgEditBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgEditBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>org_orgEditBaseBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgEditBaseBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>attachucc</property-name>
            <value>#{common_attachmentUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>org_orgDismissBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgDismissBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgDismissListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgDismissListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgChangeBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgChangeBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgLineBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgLineBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgUniteBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgUniteBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>org_orgSortBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgSortBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>org_orgAreaBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgAreaBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
      <managed-bean>
        <managed-bean-name>org_orgAreaListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgAreaListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/org/info/AddOrg.jsp</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/org/info/OrgEditInfo.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/employee/PersonQuery.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ParentClose</from-outcome>
            <to-view-id>/pages/common/CommonParentClose.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/org/info/OrgInfoDetailList.jsp</from-view-id>
        <navigation-case>
            <from-outcome>info</from-outcome>
            <to-view-id>/org/info/OrgInfoDetail.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/org/info/OrgInfoDetail.jsp</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/org/info/OrgInfoDetailList.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/org/info/OrgDismiss.jsp</from-view-id>
        <navigation-case>
            <from-outcome>close</from-outcome>
            <to-view-id>/org/DismissClose.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>/org/OrgArea.jsp</from-view-id>
        <navigation-case>
            <from-outcome>close</from-outcome>
            <to-view-id>/org/DismissClose.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/org/import/OrgDataUpload.jsp</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/org/import/OrgDataUpload.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>download</from-outcome>
            <to-view-id>/org/DownloadTemplate.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>orgInfoList</from-outcome>
            <to-view-id>/org/import/OrgDataUploadList.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/org/import/OrgDataUploadList.jsp</from-view-id>
        <navigation-case>
            <from-outcome>upload</from-outcome>
            <to-view-id>/org/import/OrgDataUpload.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
