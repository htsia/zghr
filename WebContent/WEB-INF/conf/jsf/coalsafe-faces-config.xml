<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
   <managed-bean>
        <managed-bean-name>cocalAmerceStdSetBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.coalsafe.web.CocalAmerceStdSetBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>coalsafeMgrUcc</property-name>
            <value>#{coalsafeMgrUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>selAmerceStdSetBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.coalsafe.web.SelAmerceStdBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>coalsafeMgrUcc</property-name>
            <value>#{coalsafeMgrUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>coalOverseeMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.coalsafe.web.CoalOverseeMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>coalsafeMgrUcc</property-name>
            <value>#{coalsafeMgrUcc}</value>
        </managed-property>
         <managed-property>
            <property-name>coalOverseeUcc</property-name>
            <value>#{coalOverseeUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>coalTaskMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.coalsafe.web.CoalTaskMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>coalTaskMgrUcc</property-name>
            <value>#{coalTaskMgrUcc}</value>
        </managed-property>
        <managed-property>
            <property-name>coalOverseeUcc</property-name>
            <value>#{coalOverseeUcc}</value>
        </managed-property>
    </managed-bean>
    <navigation-rule>
    	<from-view-id>/coalsafe/amerceStdDirEidt.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/pages/common/SuccessClosePop.jsf</to-view-id>
    	</navigation-case>
    </navigation-rule>
	
    <navigation-rule>
    	<from-view-id>/coalsafe/amerceStdEdit.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/pages/common/SuccessClosePop.jsf</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/coalsafe/amerceEdit.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/pages/common/SuccessClosePop.jsf</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/coalsafe/taskEdit.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/pages/common/SuccessClosePop.jsf</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/coalsafe/taskAmerce.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/pages/common/SuccessClosePop.jsf</to-view-id>
    	</navigation-case>
    </navigation-rule>
</faces-config>
