<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<managed-bean>
        <managed-bean-name>regSystemMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.RegSystemMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>regProductMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.RegProductMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SendProdouctMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.SendProdouctMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ReceriveProdouctMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.ReceriveProductBankBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DaexMonitorMgrBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.DaexMonitorMgrBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>daexloginBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.run.web.DaexLoginBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>daexSystemUcc</property-name>
           <value>#{daexSystemUcc}</value>
        </managed-property>
    </managed-bean>
     <navigation-rule>
        <from-view-id>/run/dataexchange/systemEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/run/dataexchange/productEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/run/dataexchange/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/run/dataexchange/main.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/run/dataexchange/uploadProduct.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>

