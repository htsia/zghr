<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

    <managed-bean>
      <managed-bean-name>lead_surveyAnaBB</managed-bean-name>
      <managed-bean-class>com.hr319wg.lead.web.SurveyAnalyseBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>survyucc</property-name>
            <value>#{emp_SurveyUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>leadanaucc</property-name>
            <value>#{lead_anaUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>lead_personListBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.lead.web.PersonListBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>personucc</property-name>
            <value>#{emp_personUCC}</value>
        </managed-property>
         <managed-property>
             <property-name>userqryucc</property-name>
             <value>#{user_userQryUccImpl}</value>
         </managed-property>
         <managed-property>
             <property-name>queryucc</property-name>
             <value>#{qry_QueryUCCImpl}</value>
         </managed-property>
         <managed-property>
             <property-name>orgucc</property-name>
             <value>#{org_orgUCC}</value>
         </managed-property>
         <managed-property>
             <property-name>userreportucc</property-name>
             <value>#{user_userReportUccImpl}</value>
         </managed-property>
         <managed-property>
             <property-name>shortmessageucc</property-name>
             <value>#{sys_SMUCC}</value>
         </managed-property>
        <managed-property>
            <property-name>codeitemucc</property-name>
            <value>#{sys_codeItemUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
      <managed-bean-name>lead_QueryDeptPerInfo1bb</managed-bean-name>
      <managed-bean-class>com.hr319wg.lead.web.QueryDeptPerInfoBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>ideptperinfoucc</property-name>
            <value>#{lead_ideptperinfoucc}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
      <managed-bean-name>lead_publishbb</managed-bean-name>
      <managed-bean-class>com.hr319wg.lead.web.PublishBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>createData</property-name>
            <value>#{lead_createData}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>lead_unitBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.lead.web.LeadUnitBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>leadunitucc</property-name>
            <value>#{lead_UnitUcc}</value>
        </managed-property>
    </managed-bean>

	<managed-bean>
		<managed-bean-name>gridget</managed-bean-name>
		<managed-bean-class>
			com.hr319wg.lead.web.GridGetBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>fmtmaintenanceucc</property-name>
            <value>#{lead_ifmtmaintenanceUCC}</value>
        </managed-property>
		<managed-property>
			<property-name>gridgetucc</property-name>
			<value>#{lead_igridgetUCC}</value>
		</managed-property>
        <managed-property>
            <property-name>gridgetucc</property-name>
            <value>#{lead_igridgetUCC}</value>
        </managed-property>
	</managed-bean>

    <managed-bean>
        <managed-bean-name>graphget</managed-bean-name>
        <managed-bean-class>
            com.hr319wg.lead.web.GraphGetBackingBean
        </managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>fmtmaintenanceucc</property-name>
            <value>#{lead_ifmtmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>graphgetucc</property-name>
            <value>#{lead_igraphgetUCC}</value>
        </managed-property>
    </managed-bean>

	<managed-bean>
		<managed-bean-name>textget</managed-bean-name>
		<managed-bean-class>
			com.hr319wg.lead.web.TextGetBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>textgetucc</property-name>
			<value>#{lead_itextgetUCC}</value>
		</managed-property>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>fmtmaintenanceucc</property-name>
            <value>#{lead_ifmtmaintenanceUCC}</value>
        </managed-property>
	</managed-bean>

    <managed-bean>
        <managed-bean-name>leadself</managed-bean-name>
        <managed-bean-class>
            com.hr319wg.lead.web.LeadSelfBackingBean
        </managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>textgetucc</property-name>
            <value>#{lead_itextgetUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>fmtmaintenanceucc</property-name>
            <value>#{lead_ifmtmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>activeapi</property-name>
            <value>#{sys_activePageApi}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
    <managed-bean-name>leadfmtmaintenanceBackingBean</managed-bean-name>
    <managed-bean-class>com.hr319wg.lead.web.FmtMaintenanceBackingBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
        <property-name>fmtmaintenanceucc</property-name>
        <value>#{lead_ifmtmaintenanceUCC}</value>
    </managed-property>
      <managed-property>
          <property-name>resmaintenanceucc</property-name>
          <value>#{lead_iresmaintenanceUCC}</value>
      </managed-property>
    </managed-bean>

    <managed-bean>
    <managed-bean-name>gridmaintenanceBackingBean</managed-bean-name>
    <managed-bean-class>com.hr319wg.lead.web.GridMaintenanceBackingBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
          <property-name>fmtmaintenanceucc</property-name>
          <value>#{lead_ifmtmaintenanceUCC}</value>
      </managed-property>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
    <managed-bean-name>chartmaintenanceBackingBean</managed-bean-name>
    <managed-bean-class>com.hr319wg.lead.web.ChartMaintenanceBackingBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
          <property-name>fmtmaintenanceucc</property-name>
          <value>#{lead_ifmtmaintenanceUCC}</value>
      </managed-property>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
    <managed-bean-name>sqlmaintenanceBackingBean</managed-bean-name>
    <managed-bean-class>com.hr319wg.lead.web.SqlMaintenanceBackingBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
          <property-name>fmtmaintenanceucc</property-name>
          <value>#{lead_ifmtmaintenanceUCC}</value>
      </managed-property>
        <managed-property>
            <property-name>resmaintenanceucc</property-name>
            <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>resmaintenanceBackingBean</managed-bean-name>
        <managed-bean-class>com.hr319wg.lead.web.ResMaintenanceBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>resmaintenanceucc</property-name>
        <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>htmlBackingBean</managed-bean-name>
        <managed-bean-class>com.hr319wg.lead.web.HtmlTextBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
        <property-name>resmaintenanceucc</property-name>
        <value>#{lead_iresmaintenanceUCC}</value>
        </managed-property>
        <managed-property>
        <property-name>htmltext</property-name>
        <value>#{self_htmlucc}</value>
        </managed-property>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/lead/HtmlFileEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/LeadResourceEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/lead/TextResMaintence.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/ChartResMaintence.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/GridResEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/ChartResEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/LeadSQLEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/UnitList.jsp</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/lead/UnitEdit.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/lead/UnitList.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lead/UnitEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/lead/UnitList.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/desktop/RunQuery.jsp</from-view-id>
        <navigation-case>
            <from-outcome>queryresult</from-outcome>
            <to-view-id>/desktop/QueryResult.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>
