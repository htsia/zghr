<navigation-rule>
  <from-view-id>/*</from-view-id>
  
   <navigation-case>
   <from-outcome>self_PayQuery_jsf</from-outcome>
   <to-view-id>/self/PayQuery.jsf</to-view-id>
   </navigation-case>
   
   <navigation-case>
   <from-outcome>self_PayOneQuery_jsf</from-outcome>
   <to-view-id>/self/PayOneQuery.jsf</to-view-id>
   </navigation-case>
   
   <navigation-case>
   <from-outcome>self_PayQueryFalse_jsf</from-outcome>
   <to-view-id>/self/PayQueryFalse.jsf</to-view-id>
   </navigation-case>
   

    <navigation-case>
   <from-outcome>self_ManpowerCostQuery_jsf</from-outcome>
   <to-view-id>/self/ManpowerCostQuery.jsf</to-view-id>
   </navigation-case>
   
 <navigation-case>
   <from-outcome>self_ManpowerCostAllQuery_jsf</from-outcome>
   <to-view-id>/self/ManpowerCostAllQuery.jsf</to-view-id>
   </navigation-case>
   
</navigation-rule>

    <navigation-rule>
        <from-view-id>/self/PersonContact.jsp</from-view-id>
        <navigation-case>
                <from-outcome>self_PersonContactUpdate</from-outcome>
                <to-view-id>/self/PersonContactUpdate.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/self/PersonContactUpdate.jsp</from-view-id>
        <navigation-case>
                <from-outcome>self_PersonContact</from-outcome>
                <to-view-id>/self/PersonContact.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/self/PersonContact.jsp</from-view-id>
        <navigation-case>
                <from-outcome>self_PersonContactUpdate</from-outcome>
                <to-view-id>/self/PersonInfoDetail.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
            <from-view-id>/self/PersonInfoDetail.jsp</from-view-id>
            <navigation-case>
                <from-outcome>list</from-outcome>
                <to-view-id>/self/PersonInfoDetailList.jsf</to-view-id>
            </navigation-case>
    </navigation-rule>

    <navigation-rule>
            <from-view-id>/self/AddAdvices.jsp</from-view-id>
            <navigation-case>
                <from-outcome>success</from-outcome>
                <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
            </navigation-case>
    </navigation-rule>


    <navigation-rule>
            <from-view-id>/self/info/BatchModifyInfo.jsp</from-view-id>
            <navigation-case>
                <from-outcome>success</from-outcome>
                <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
            </navigation-case>
    </navigation-rule>

    <navigation-rule>
            <from-view-id>/self/info/InsertInfo.jsp</from-view-id>
            <navigation-case>
                <from-outcome>success</from-outcome>
                <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
            </navigation-case>
    </navigation-rule>

     <navigation-rule>
            <from-view-id>/self/AddAnswer.jsp</from-view-id>
            <navigation-case>
                <from-outcome>success</from-outcome>
                <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
            </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/self/SelfInfo2.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/inProcess.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/hqlead/HQSelfInfo.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/inProcess.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
       <from-view-id>/self/PersonInfoDetailList.jsp</from-view-id>
        <navigation-case>
                <from-outcome>info</from-outcome>
                <to-view-id>/self/PersonInfoDetail.jsf</to-view-id>
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/self/PersonInfoDetail.jsp</from-view-id>
            <navigation-case>
                <from-outcome>list</from-outcome>
                <to-view-id>/self/PersonInfoDetailList.jsf</to-view-id>
            </navigation-case>
    </navigation-rule>

 <application>
  <view-handler>org.apache.myfaces.application.jsp.JspTilesViewHandlerImpl</view-handler>
  <locale-config/>
 </application>

