<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
    <managed-bean>
      <managed-bean-name>self_surveyBB</managed-bean-name>
      <managed-bean-class>com.hr319wg.self.web.SurveyBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
            <property-name>surveyucc</property-name>
            <value>#{self_surveyUCC}</value>
      </managed-property>
    </managed-bean>

        <managed-bean>
          <managed-bean-name>self_surveyprocessBB</managed-bean-name>
          <managed-bean-class>com.hr319wg.self.web.SurveyProcessBackingBean</managed-bean-class>
          <managed-bean-scope>request</managed-bean-scope>
          <managed-property>
                <property-name>surveyucc</property-name>
                <value>#{self_surveyUCC}</value>
          </managed-property>
            <managed-property>
                  <property-name>userreportucc</property-name>
                  <value>#{user_userReportUccImpl}</value>
            </managed-property>

        </managed-bean>

    <managed-bean>
      <managed-bean-name>sys_surveyBB</managed-bean-name>
      <managed-bean-class>com.hr319wg.sys.web.SurveyBackingBean</managed-bean-class>
