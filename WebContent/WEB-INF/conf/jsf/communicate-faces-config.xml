      <managed-bean-class>com.hr319wg.sys.web.QuestionBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
            <property-name>surveyucc</property-name>
            <value>#{self_surveyUCC}</value>
      </managed-property>
    </managed-bean>
    <managed-bean>
      <managed-bean-name>self_QuestionEditBB</managed-bean-name>
      <managed-bean-class>com.hr319wg.sys.web.QuestionEditBackingBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
            <property-name>forumucc</property-name>
