<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
     <managed-bean>
        <managed-bean-name>orgTemplateBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.OrgTemplateBackBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>templateucc</property-name>
            <value>#{orgtemplateucc}</value>
        </managed-property>
        <managed-property>
            <property-name>postucc</property-name>
            <value>#{post_postUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>fastcreatorgBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.FasterCreateOrgBankBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>templateucc</property-name>
            <value>#{orgtemplateucc}</value>
        </managed-property>
        <managed-property>
            <property-name>postucc</property-name>
            <value>#{post_postUCC}</value>
        </managed-property>
        <managed-property>
            <property-name>orgucc</property-name>
            <value>#{org_orgUCC}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>orgtemppostlistBB</managed-bean-name>
        <managed-bean-class>com.hr319wg.org.web.ShowOrgTemPostListBankBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>templateucc</property-name>
            <value>#{orgtemplateucc}</value>
        </managed-property>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/org/orgTemplateEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/org/OrgTemplateItemEdit.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/common/SuccessClosePop.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
